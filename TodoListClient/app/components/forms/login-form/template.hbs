<form {{on 'submit' this.authenticate }}>
  <label for="identification">Login</label>
  <Input id="identification" @type="text" placeholder="Enter Login" @value={{this.identification}} />
  <label for="password">Password</label>
  <Input id="password" placeholder="Enter Password" @type="password" @value={{this.password}} />
  <button type="submit">Login</button>
</form>